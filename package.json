{
  "name": "angularjs-cli",
  "version": "1.0.0",
  "description": "AngularJS CLI to create TypeScript driven projects and manage components",
  "main": "src/main.ts",
  "directories": {
    "src": "src",
    "test": "test"
  },
  "bin": {
    "ngjs": "./bin/ngjs.js"
  },
  "scripts": {
    "clean": "rimraf dist",
    "serve": "webpack -p --progress",
    "build": "cross-env NODE_ENV=production webpack -p --progress",
    "lint:js": "tslint --project tslint.json src/**/*.ts",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scipper/angularjs-cli.git"
  },
  "keywords": [
    "angular",
    "angularjs",
    "cli"
  ],
  "author": "Steffen Kowalski <scipper@myscipper.de>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/scipper/angularjs-cli/issues"
  },
  "homepage": "https://github.com/scipper/angularjs-cli#readme",
  "dependencies": {
    "complete": "0.4.4",
    "fs": "0.0.1-security",
    "lodash": "4.17.4",
    "path": "0.12.7",
    "prompt-sync": "4.1.5",
    "mkdirp": "0.5.1"
  },
  "devDependencies": {
    "@types/lodash": "4.14.80",
    "@types/mkdirp": "0.5.1",
    "@types/node": "9.4.6",
    "clean-webpack-plugin": "0.1.17",
    "cross-env": "5.0.2",
    "license-webpack-plugin": "1.1.1",
    "progress-bar-webpack-plugin": "1.10.0",
    "rimraf": "2.6.1",
    "ts-loader": "3.1.1",
    "tslint": "5.8.0",
    "typescript": "2.6.1",
    "webpack": "3.8.1"
  }
}
